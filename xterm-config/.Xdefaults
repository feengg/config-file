!! This is Xdefaults to make xterm look good
! After changing contents, run "xrdb -merge ~/.Xdefaults"

xterm*background:	    black
xterm*foreground:	    green
!xterm*font:		    -bitstream-bitstream vera sans mono-*-r-normal-*-16-*-*-*-*-0-iso10646-1
xterm*faceName:		    'Mono'
xterm*faceSize:		    14
!xterm*iconPixmap:	    /usr/share/pixmaps/gonme-gemvt.xbm
!xterm*iconMask:		    /usr/share/pixmaps/gonme-gemvt-mask.xbm
!xterm*iconName:	    hywei-xterm
xterm*loginShell:	    true
xterm*cursorColor:	    rgb:00/80/00
xterm*borderColor:	    white
xterm*scrollColor:	    black
xterm*visualBell:	    true
xterm*saveLines:	    1024
xterm*allowSendEvents:	    true
xterm*sessionMgt:	    false
xterm*rightScrollBar:	    true
!color
xterm*colorUL:		    yellow
xterm*colorBD:		    white


